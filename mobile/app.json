{
  "expo": {
    "name": "Nabha Health App",
    "slug": "nabha-health-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E7D8F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nabhahealth.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2E7D8F"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nabhahealth.app",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CALL_PHONE",
        "SEND_SMS",
        "RECEIVE_SMS",
        "READ_SMS",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [

      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location for emergency services."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "This app needs access to camera for capturing medical photos."
        }
      ]
    ]
  }
}
